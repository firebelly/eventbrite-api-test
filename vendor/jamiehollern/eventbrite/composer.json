{
  "name": "jamiehollern/eventbrite",
  "description": "A PHP library to provide interactions with the Eventbrite API.",
  "type": "library",
  "license": "GPL2",
  "keywords": [
    "eventbrite",
    "event",
    "rest"
  ],
  "authors": [
    {
      "name": "Jamie Hollern",
      "email": "jamiehollern@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "~6.0",
    "guzzlehttp/psr7": "~1.0",
    "guzzlehttp/promises": "~1.0",
    "mtdowling/jmespath.php": "~2.2"
  },
  "require-dev": {
    "ext-curl": "*",
    "psr/log": "~1.0",
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "monolog/monolog": "~1.13",
    "satooshi/php-coveralls": "dev-master",
    "oodle/krumo": "*"
  },
  "autoload": {
    "psr-4": {
      "jamiehollern\\eventbrite\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "jamiehollern\\eventbrite\\Tests\\": "tests/"
    }
  }
}
